{
  "name": "panel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "debounce": "^1.2.0",
    "howler": "^2.1.3",
    "jalali-moment": "^3.3.3",
    "moment-timezone": "^0.5.28",
    "uikit": "^3.3.6",
    "vee-validate": "^3.2.5",
    "vue": "^2.6.11",
    "vue-debounce": "^2.5.1",
    "vue-router": "^3.1.6",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.1.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "rtlcss": {
        "stringMap": [
          {
            "name": "previous-next",
            "priority": 100,
            "search": [
              "previous",
              "Previous",
              "PREVIOUS"
            ],
            "replace": [
              "next",
              "Next",
              "NEXT"
            ],
            "options": {
              "scope": "*",
              "ignoreCase": false
            }
          }
        ]
      }
    }
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.3",
    "@vue/cli-plugin-eslint": "~4.2.3",
    "@vue/cli-service": "~4.2.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "rtlcss": "^2.4.1",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
